<!-- BEGIN_INCLUDE(anaplan:config-auth-basic) -->
	<anaplan:config-auth-basic 
		name="anaplan-default-auth" 
		username="someUserName"
		password="somePassword" />
<!-- END_INCLUDE(anaplan:config-auth-basic) -->

<!-- BEGIN_INCLUDE(anaplan:config-auth-cert) -->
	<anaplan:config-auth-cert name="anaplan-cert-auth" ... />
<!-- END_INCLUDE(anaplan:config-auth-cert) -->

<!-- BEGIN_INCLUDE(anaplan:importToModel) -->
	<anaplan:importToModel
		data="#[header:data]" 
		workspaceId="#[header:workspaceId]" 
		modelId="#[header:modelId]" 
		importId="#[header:importId]" 
		columnSeparator="#[header:columnSeparator]" 
		delimiter="#[header:delimiter]" />
<!-- END_INCLUDE(anaplan:importToModel) -->

<!-- BEGIN_INCLUDE(anaplan:exportFromModel) -->
	<anaplan:exportFromModel
		workspaceId="#[header:workspaceId]" 
		modelId="#[header:modelId]" 
		exportId="#[header:exportId]" />
<!-- END_INCLUDE(anaplan:exportFromModel) -->

<!-- BEGIN_INCLUDE(anaplan:deleteFromModel) -->
	<anaplan:executeAction
		workspaceId="#[header:workspaceId]" 
		modelId="#[header:modelId]" 
		actionId="#[header:deleteActionId]" />
<!-- END_INCLUDE(anaplan:deleteFromModel) -->

<!-- BEGIN_INCLUDE(anaplan:runProcess) -->
	<anaplan:runProcess
		workspaceId="#[header:workspaceId]" 
		modelId="#[header:modelId]" 
		actionId="#[header:processId]" />
<!-- END_INCLUDE(anaplan:runProcess) -->
